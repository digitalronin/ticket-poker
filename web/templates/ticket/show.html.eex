<h2>Show ticket</h2>

<ul>

  <li>
    <strong>Url:</strong>
    <%= @ticket.url %>
  </li>

  <li>
    <strong>Point options:</strong>
    <%= @ticket.point_options %>
  </li>

  <li>
    <strong>Estimates:</strong>
    <%= @ticket.estimates %>
  </li>

  <li>
    <strong>Team:</strong>
    <%= @ticket.team_id %>
  </li>

</ul>

<%= link "Edit", to: ticket_path(@conn, :edit, @ticket) %>
<%= link "Back", to: ticket_path(@conn, :index) %>
